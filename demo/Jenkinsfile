    steps {
   echo "Gradle..."
         
        sh """#!/bin/bash
               cd ${WORKSPACE}/demo
               gradle clean test --no-daemon
               
                """
        
        
    }
        }
    }
    post { 
        always { 
           
             echo 'DONE!'
        }
    }
}
